<style>
#themepage h1 { text-align: center; line-height: 1em; margin: 0 0 10px 0; }
#themepage h2 { text-align: center; font-size: 1.3em; line-height: 1em; color: #aaa; margin: 0; }

#themepage #theme { font-size: 100%; }
#themepage #theme #themecontainer { padding: 40px 0; overflow-x: scroll; }

#themepage #installtheme { text-align: center; }
#themepage #installtheme a { display: inline-block; padding: 10px 14px; margin: 10px 0 20px 0; border-radius: 4px; color: #fff; background: #5c1f87; }
#themepage #installtheme a:hover { background: #421662; }

#themepage #themepp { text-align: center; }

#themepage #theme #border #window {
  background: rgba(229, 230, 226, 0.592);
  width: 681px;
  padding: 0px 0px;
  margin: 0 auto;
  overflow:hidden;
  border-radius: 7px;
}
#themepage #theme #shadow {
  width: 681px;
  padding: 0;
  margin: 0 auto;
  background: rgba(0, 0, 0, 0.18);
  box-shadow: 0 0 22px rgba(0, 0, 0, 0.18);
  border-radius: 7px;
  -webkit-user-select: none;
  user-select: none;
}

#themepage #theme #query img {
  float: right;
  padding: 0 10px 0 0;
  height: 65px;
}  
#themepage #theme #query h3 {
  background: rgba(229, 230, 226, 0);
  padding: 15px 10px;
  margin: 0 85px 0px 0;
  font-family: -apple-system, "Helvetica Neue";
  font-weight: normal;
  font-size: 35px;
  line-height: 1em;
  color: rgba(0, 0, 0, 1);
  -webkit-user-select: text;
  user-select: text;
}
#themepage #theme #query h3::selection {
  background: rgba(162, 192, 220, 1);
}
#themepage #theme #results {
  position: relative;
  border-top: solid 0px rgba(203, 203, 203, 0.953);
}
#themepage #theme .result {
  position: relative;
  height: 43px;
  padding: 0;
  margin: 0;
  border-bottom: solid 0px rgba(203, 203, 203, 0.953);
  white-space: nowrap;
}
#themepage #theme .result.selected {
  background: rgba(23, 112, 229, 1);
}
#themepage #theme .result img {
  position: absolute;
  left: 7px;
  top: 7.5px;
  max-width: 28px;
}
#themepage #theme .result h3 {
  position: absolute;
  left: 42px;
  top: 5px;
  margin: 0;
  padding: 0;
  line-height: 1.2em;
  font-family: -apple-system, "Helvetica Neue";
  font-weight: normal;
  font-size: 15px;
  color: rgba(52, 53, 50, 1);
  width: 590px;
  overflow: hidden;
  text-overflow: ellipsis;
}
#themepage #theme .result.selected h3 {
  color: rgba(252, 255, 255, 1);
}
#themepage #theme .result p {
  position: absolute;
  left: 42px;
  top: 24px;
  margin: 0;
  padding: 0;
  line-height: 1.2em;
  font-family: -apple-system, "Helvetica Neue";
  font-weight: normal;
  font-size: 12px;
  color: rgba(92, 90, 94, 1);
  width: 590px;
  overflow: hidden;
  text-overflow: ellipsis;
}
#themepage #theme .result.selected p {
  color: rgba(255, 255, 255, 0.796);
}
#themepage #theme .result .shortcut {
  position: absolute;
  right: 7px;
  padding: 0 4px;
  font-family: -apple-system, "Helvetica Neue";
  font-weight: normal;
  font-size: 16px;
  color: rgba(255, 255, 255, 0.494);
  line-height: 43px;
  vertical-align: middle;
}
#themepage #theme .result.selected .shortcut {
  color: rgba(255, 255, 255, 1);
}
#themepage #theme #results #scrollbar {
  position: absolute;
  top: 0;
  right: 0;
  width: 2px;
  height: 70px;
  background: rgba(53, 129, 136, 0);
}
</style>
